{
  "name": "AuroraRP",
  "version": "1.0.18",
  "description": "A launcher for AuroraRP.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win",
    "rebuild": "electron-rebuild -f -w keytar"
  },
  "keywords": [
    "aurorarp",
    "aurorax",
    "aurora"
  ],
  "author": "Curt",
  "license": "ISC",
  "build": {
    "appId": "AurorRP",
    "productName": "AurorRP",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/uninstall.ico",
      "oneClick": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true,
      "createStartMenuShortcut": true,
      "packElevateHelper": true,
      "createDesktopShortcut": true,
      "shortcutName": "AurorRP",
      "license": "eula.txt"
    }
  },
  "dependencies": {
    "electron-dl": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.1.2",
    "nan": "^2.14.1",
    "node-gyp": "^6.1.0",
    "node-tcp-proxy": "0.0.15",
    "randomstring": "^1.1.5",
    "udp-proxy": "^1.2.0",
    "windows-build-tools": "^5.2.2"
  }
}
